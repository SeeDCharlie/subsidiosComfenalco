
//prueba para registrar un usuario via POST
curl -X POST -H "Content-Type: application/json" -d '{"nombre": "carlos" ,"apellido": "asda","fechaNacimiento" : "1998-02-16","idGnr" : 2,"numeroDocumento" : 6516516,"email" : "correo@correo.com","idCiudad" : 1,"idPais" : 1,"idTipoDoc" : 1,"password": "password","idTipoUsr": 1}' http://127.0.0.1:8000/UserRegistration

// devuelve todos los usuarios registrados
curl http://127.0.0.1:8000/getAllUsers

// devuelve todos los programas
curl http://127.0.0.1:8000/programas

// devuelve los datos del usuario segun el id 
curl -X POST -H "Content-Type: application/json" -d '{"id":6}' http://127.0.0.1:8000/getUserByid


// registro de solicitud a un programa de subsidio
curl -X POST -H "Content-Type: application/json" -d '{"idUsr":6, "idPrograma":2, "fechaFinalizacion":"2021-03-05"}' http://127.0.0.1:8000/registrarSubsidio

curl -X POST -H "Content-Type: multipart/form-data" -F "uploaded_file=@/home/seed/Imágenes/fp.jpg" -F "uploadForm=true" -F "id=1" https://comfenalcosubsidios.seedcharlie.co/uploadFile.php
// cargar formulario para el registro de un subsidio
curl -X POST -H "Content-Type: multipart/form-data" -F "uploadForm=true" https://comfenalcosubsidios.seedcharlie.co/uploadFile.php
curl -F "uploaded_file=asdsd.jpg" -F "uploadForm=true" -F "id=1" -F "data=@/home/seed/Imágenes/fp.jpg" https://comfenalcosubsidios.seedcharlie.co/uploadFile.php




