// registro de de un anexo
URL : /registrarAnexo
metodo POST
DATOS:

{
    "estado":"activo",
    "observaciones":"observacion tres",
    "idSubsidio":27,
    "idProgRequerimiento":1,
    "nombreArchivo":"fileTestdos.pdf",
    "documento":"url"
}

-------------------------------------------------------------- ///////// --------------------------------------------------------------
// actualizar solicitudes de subsidio

url : /actualizarSubsidio
metodo PUT
#datos obligatorios

idSubsidio: Integer
idEstado: Integer
idUsr: Integer
idPrograma: Integer
fechaFinalizacion: Date(YYYY-MM-DD)

-------------------------------------------------------------- ///////// --------------------------------------------------------------
