// devuelve los datos de los subsidios como el estado, datos del programa y usuario. (Recibe idUser: 2)
curl -X POST -H "Content-Type: application/json" https://comfenalcosubsidios.seedcharlie.co/getUserSubsidios

-------------------------------------------------------------- ///////// --------------------------------------------------------------
// registro de solicitud a un programa de subsidio
curl -X POST -H "Content-Type: application/json" -d '{"idUsr":6,"nombreArchivo":"form.pdf", "idPrograma":2, "fechaFinalizacion":"2021-03-05"}' 
https://comfenalcosubsidios.seedcharlie.co/registrarSubsidio

-------------------------------------------------------------- ///////// --------------------------------------------------------------
// registro de de un anexo
URL : /registrarAnexo
metodo POST
DATOS:
{
    "estado":"estadoPrueba",
    "observaciones":"observacion prueba",
    "idSubsidio":6,
    "idProgRequerimiento":1,
    "nombreArchivo":"fileTest.pdf"
}


-------------------------------------------------------------- ///////// --------------------------------------------------------------
// actualizar solicitudes de subsidio

url : /actualizarSubsidio
metodo PUT
#datos obligatorios

idSubsidio: Integer
idEstado: Integer
idUsr: Integer
idPrograma: Integer
fechaFinalizacion: Date(YYYY-MM-DD)

-------------------------------------------------------------- ///////// --------------------------------------------------------------
// actualizar datos de un usuario

url : /actualizarUsuario
metodo PUT
#datos obligatorios

{
    "isUsr":56,
    "nombre": "esteban" ,
    "apellido": "asda",
    "fechaNacimiento" : "1998-02-16",
    "idGnr" : 3,
    "numeroDocumento" : 888,
    "email" : "hellotumaasma@correo.com",
    "idCiudad" : 1,
    "idPais" : 55555,
    "idTipoDoc" : 1,
    "password": "password",
    "idTipoUsr": 1
}
-------------------------------------------------------------- ///////// --------------------------------------------------------------
// eliminar un usuario
metodo DELETE
url : /eliminarUsuario
#datos obligatorios

idUsuario: Integer

Ejemplo: /eliminarUsuario?idUsuario=56

-------------------------------------------------------------- ///////// --------------------------------------------------------------
// actualizar un anexo
metodo PUT
URL : /actualizarAnexo
DATOS:
{   
    "idAnexo":2,
    "estado":"estadoPrueba",
    "observaciones":"observacion prueba",
    "idSubsisdio":6,
    "idProgRequerimiento":1,
    "nombreArchivo":"fileTest.pdf"
}

